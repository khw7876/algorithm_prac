def solution(order):
    # crap_num = [3,6,9]
    answer = 0
    # list_order = list(str(order))
    # for num in crap_num:
    #     if str(num) in str(order):
    #         answer += list_order.count(str(num))
    for i in str(order):
        if int(i) !=0 and int(i)%3==0:
            answer +=1
    return answer
