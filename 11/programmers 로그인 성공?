def solution(id_pw, db):
    answer = ''
    for i in db:
        if id_pw[0] == i[0]:
            if id_pw[1] == i[1]:
                answer = "".join("login")
                break;
            else:
                answer = "".join("wrong pw")
                break;
        else :
            if answer == "":
                answer = "".join("fail")
        
    return answer
